<Window x:Class="SampleWPF_project.Ancillary"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SampleWPF_project"
        mc:Ignorable="d"
        Title="Ancillary" Height="450" Width="800">
    <Grid>
        <TabControl Name="Tabs1" TabStripPlacement="Top" Background="LightGray" BorderBrush="Blue" Margin="10,10,0,5">
            <TabItem Header="Ancillary" IsSelected="True" Background="Gray">
                <Grid Background="{DynamicResource {x:Static SystemColors.ControlBrushKey}}" RenderTransformOrigin="0.507,0.496">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="400*"/>
                        <ColumnDefinition Width="400*"/>
                        <ColumnDefinition Width="200*"/>
                        <ColumnDefinition/>
                    </Grid.ColumnDefinitions>

                    <Grid.RowDefinitions>
                        <RowDefinition Height="50" />
                        <RowDefinition Height="170" />
                        <RowDefinition Height="170"/>
                        <RowDefinition Height="170"/>
                        <RowDefinition Height="100"/>
                        <RowDefinition Height="70"/>
                        <RowDefinition Height="70"/>
                    </Grid.RowDefinitions>

                    <StackPanel Grid.Row="0" Orientation="Vertical" Margin="10,0,10,10">
                        <Label Content="Ancillary Vendor Name:" FontWeight="Bold"/>
                        <TextBox Text="{Binding SelectedProviderAccount.FacilityName, Mode=TwoWay}"/>
                    </StackPanel>

                    <StackPanel Grid.Row="0" Grid.Column="1" Orientation="Horizontal"  HorizontalAlignment="Center" >
                        <StackPanel Grid.Row="0" Grid.Column="1" Orientation="Vertical" Margin="10,0,10,10">
                            <Label Content="Provider Type:" FontWeight="Bold"/>
                            <ComboBox ItemsSource="{Binding ProviderType, Mode=TwoWay}" SelectedItem="{Binding SelectedProviderAccount.ProviderType}" DisplayMemberPath="Description"/>
                        </StackPanel>

                        <StackPanel Grid.Row="0" Grid.Column="1" Orientation="Vertical" Margin="10,0,10,10">
                            <Label Content="Medicare ID:" FontWeight="Bold"/>
                            <TextBox Text="{Binding SelectedProviderAccount.MedicareID, Mode=TwoWay}"/>
                        </StackPanel>

                        <StackPanel Grid.Row="0" Grid.Column="1" Orientation="Vertical" Margin="10,0,10,10">
                            <Label Content="License Number:" FontWeight="Bold"/>
                            <TextBox Text="{Binding SelectedProviderAccount.PractitionerLicenseNumber, Mode=TwoWay}"/>
                        </StackPanel>
                    </StackPanel>

                    <StackPanel Grid.Row="0" Grid.Column="2" Orientation="Vertical" Margin="10,0,20,10">
                        <Label Content="DEA Number:" FontWeight="Bold"/>
                        <TextBox Text="{Binding SelectedProviderAccount.DEANumber, Mode=TwoWay}"/>
                    </StackPanel>
                    <DataGrid x:Name="gridView" Margin="10,17,15,10" Grid.Row="1" Grid.ColumnSpan="3" AutoGenerateColumns="False" CanUserDeleteRows="True" CanUserAddRows="True" 
                      ItemsSource="{Binding SelectedProviderTINGroupAndInfo}">
                        <DataGrid.Columns>
                            <DataGridTextColumn Binding="{Binding TaxIDNumber}" Header="TIN" />
                            <DataGridTextColumn Binding="{Binding GroupName}" Header="DBA" />
                            <DataGridTextColumn Binding="{Binding GroupNPI}" Header="Facility NPI" />
                            <DataGridComboBoxColumn ItemsSource="{Binding PricingFlag}" SelectedValueBinding="{Binding LevelOfPricing}"  SelectedValuePath="Flag" DisplayMemberPath="Description"  Header="Pricing">
                            </DataGridComboBoxColumn>
                            <DataGridTextColumn Binding="{Binding EffectiveDate}" Header="Effective" />
                            <DataGridTextColumn Binding="{Binding TermDate}" Header="Term" />
                        </DataGrid.Columns>
                    </DataGrid>


                    <StackPanel Grid.Row="2" Grid.ColumnSpan="2">
                        <Grid>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition/>
                                <ColumnDefinition/>
                                <ColumnDefinition/>
                            </Grid.ColumnDefinitions>

                            <StackPanel Grid.Row="0" Grid.Column="0" Height="160">
                                <Label Content="Specialities:" Margin="10,0,10,0" FontWeight="Bold"/>
                                <DataGrid Height="130" x:Name="gridView11" ItemsSource="{Binding SelectedProviderSpecialties}" Margin="10,0,15,0" 
                                  AutoGenerateColumns="False">
                                    <DataGrid.Columns>
                                        <DataGridComboBoxColumn ItemsSource="{Binding Specialties}" SelectedValueBinding="{Binding Specialty}" SelectedValuePath="SpecialtyId" DisplayMemberPath="Description" Header="" />
                                    </DataGrid.Columns>
                                </DataGrid>

                            </StackPanel>

                            <StackPanel Grid.Row="0" Grid.Column="1"  Height="160">
                                <Label Content="RPGs:" Margin="10,0,10,0" FontWeight="Bold"/>
                                <DataGrid Height="130" x:Name="gridView111" ItemsSource="{Binding SelectedProviderRpgs}" Margin="10,0,15,0" 
                                  AutoGenerateColumns="False">
                                    <DataGrid.Columns>
                                        <DataGridComboBoxColumn ItemsSource="{Binding Rpgs}" SelectedValueBinding="{Binding RpgId}" SelectedValuePath="RPG_Id" DisplayMemberPath="RPGName" Header="RPG" />
                                        <DataGridCheckBoxColumn Binding="{Binding ParentContractRpg}" Header="PC" />
                                    </DataGrid.Columns>
                                </DataGrid>

                            </StackPanel>

                            <StackPanel Grid.Row="0" Grid.Column="2" Height="160">
                                <Label Content="Contract Initiative(s):" Margin="10,0,10,0" FontWeight="Bold"/>
                                <DataGrid Height="130" x:Name="gridView12" Margin="10,0,15,0" 
                                  AutoGenerateColumns="False">
                                    <DataGrid.Columns>
                                        <DataGridComboBoxColumn ItemsSource="{Binding ContractInitiatives}" SelectedValuePath="InitiativeId" DisplayMemberPath="Description" Header="" />
                                    </DataGrid.Columns>
                                </DataGrid>

                            </StackPanel>
                        </Grid>
                    </StackPanel>

                    <StackPanel Grid.Row="3" Margin="10,10,10,86" Grid.ColumnSpan="2" Grid.RowSpan="2">
                        <Label Content="Network Participation:" Margin="10,0,10,0" FontWeight="Bold"/>
                        <DataGrid Height="143" Margin="0,0,0,20" x:Name="gridView3" ItemsSource="{Binding SelectedProviderNetworkParticipation}" 
                          AutoGenerateColumns="False">
                            <DataGrid.Columns>
                                <DataGridTextColumn Binding="{Binding ProviderId}" Header="ProvID" />
                                <DataGridTextColumn Binding="{Binding NetworkId}" Header="Network ID" />
                                <DataGridTextColumn Binding="{Binding Networks.NetworkName}" Header="Network" />
                                <DataGridTextColumn Binding="{Binding RpgId}" Header="RPG_ID" />
                                <DataGridTextColumn Binding="{Binding RPG.RPGName}" Header="RPG Name" />
                                <DataGridTextColumn Binding="{Binding EffectiveDate}" Header="Effective" />
                                <DataGridTextColumn Binding="{Binding TermDate}" Header="Term" />
                            </DataGrid.Columns>
                        </DataGrid>

                    </StackPanel>

                    <Border BorderBrush="Black" BorderThickness="1"  Grid.Column="2" Grid.Row="2"  Grid.RowSpan="3" Margin="5,0,15,68"/>
                    <StackPanel Grid.Row="2" Grid.Column="2" Orientation="Vertical" Margin="5,20,15,35" Grid.RowSpan="3">
                        <Label Content="Effective Date:" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="10,0,0,0" FontWeight="Bold"/>
                        <DatePicker Text="{Binding SelectedProviderAccount.EffectiveDate, Mode=TwoWay}" Margin="10,5,25,5"/>

                        <Label Content="Term Date:" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="10,0,0,0" FontWeight="Bold"/>
                        <TextBox Text="{Binding SelectedProviderAccount.TermDate, Mode=TwoWay}" Margin="10,5,25,5"/>

                        <Label Content="Last Updated Date:" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="10,0,0,0" FontWeight="Bold"/>
                        <TextBox IsEnabled="False" Text="{Binding SelectedProviderAccount.LastUpdatedDate, Mode=TwoWay}" Margin="10,5,25,5"/>

                        <Label Content="HA Exported Failed:" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="10,0,0,0" FontWeight="Bold"/>
                        <TextBox IsEnabled="False" Text="{Binding SelectedProviderAccount.ExportFailed, Mode=TwoWay}" Margin="10,5,25,5"/>

                        <Label Content="HA Notification Date:" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="10,0,0,0" FontWeight="Bold"/>
                        <TextBox IsEnabled="False" Text="{Binding SelectedProviderAccount.HealthaxisNotificationDate, Mode=TwoWay}" Margin="10,5,25,5"/>

                        <Label Content="Exported Date:" HorizontalAlignment="lEFT" VerticalAlignment="Top" Margin="10,0,0,0" FontWeight="Bold"/>
                        <TextBox IsEnabled="False" Text="{Binding SelectedProviderAccount.ExportedDate, Mode=TwoWay}" Margin="10,5,25,5"/>
                    </StackPanel>

                    <StackPanel Grid.Row="5" Grid.Column="2" Orientation="Vertical" Margin="5,0,10,-75" >
                        <Label Content="Exclude From HA Export:" HorizontalAlignment="Left" VerticalAlignment="Top"  Margin="10,0,0,0" FontWeight="Bold"/>
                        <TextBox Text="{Binding SelectedProviderAccount.ExcludeFromHAExport, Mode=TwoWay}" Margin="10,0,25,5"/>
                        <Label Content="Account Owner:" HorizontalAlignment="Left" VerticalAlignment="Top"  Margin="10,0,0,0" FontWeight="Bold"/>
                        <ComboBox ItemsSource="{Binding AccountOwners, Mode=TwoWay}" SelectedItem="{Binding SelectedProviderAccount.AccountOwner}" DisplayMemberPath="FirstName"/>
                    </StackPanel>

                    <StackPanel Grid.Row="4" Margin="0,0,20,10" Grid.RowSpan="2" VerticalAlignment="Center">
                        <Button Content="Parent Account(s)" Command="{Binding ProviderParentAccountCommand}" Margin="10,10,30,10" Foreground="White" Background="#FFD65858" FontWeight="Bold" Height="30"/>
                    </StackPanel>

                    <StackPanel Grid.Row="5" Grid.Column="0" Margin="0,10,0,-75">
                        <Label Content="System/Group Affiliation:" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="10,0,0,0" FontWeight="Bold"/>
                        <TextBox Text="{Binding SelectedProviderAccount.SystemOrGroupAffiliation, Mode=TwoWay}" Margin="10,0,25,5"/>
                        <Label Content="Website:" HorizontalAlignment="Left" VerticalAlignment="Top"  Margin="10,0,0,0" FontWeight="Bold"/>
                        <TextBox Text="{Binding SelectedProviderAccount.Website, Mode=TwoWay}" Margin="10,0,25,5"/>
                    </StackPanel>

                    <StackPanel Grid.Row="4" Orientation="Horizontal" HorizontalAlignment="Center" Height="65" VerticalAlignment="Bottom" Grid.ColumnSpan="3" Margin="285,0,20,0" >
                        <StackPanel Grid.Row="7" Grid.Column="1" Grid.RowSpan="2" Orientation="Vertical" Margin="10,0,10,10">
                            <Label Content="Provider Count:" FontWeight="Bold"/>
                            <TextBox Text="{Binding SelectedProviderAccount.ProviderCount, Mode=TwoWay}" Width="150"/>
                        </StackPanel>

                        <StackPanel Grid.Row="7" Grid.Column="1" Orientation="Vertical" Grid.RowSpan="3" Margin="10,0,10,10">
                            <Label Content="Coverage Type:" FontWeight="Bold"/>
                            <TextBox Text="{Binding SelectedProviderAccount.CoverageType, Mode=TwoWay}" Width="150"/>
                        </StackPanel>

                        <StackPanel Grid.Row="7" Grid.Column="1" Orientation="Vertical" Grid.RowSpan="2" Margin="10,0,0,10">
                            <Label Content="PPO Provider Flag:" FontWeight="Bold"/>
                            <TextBox Text="{Binding SelectedProviderAccount.PPOProviderFlag, Mode=TwoWay}" Width="150"/>
                        </StackPanel>
                    </StackPanel>

                    <StackPanel Grid.Row="5" Grid.Column="1" Margin="0,10,0,-30">
                        <Label Content="Hospital Affiliation:" Margin="10,0,10,0" FontWeight="Bold"/>
                        <TextBox Text="{Binding SelectedProviderAccount.HospitalAffiliation, Mode=TwoWay}" Margin="10,0,10,0" />
                    </StackPanel>

                    <StackPanel Grid.Row="5" Grid.Column="1" Margin="0,70,0,-75" Orientation="Vertical">
                        <Grid>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition/>
                                <ColumnDefinition/>
                            </Grid.ColumnDefinitions>

                            <Border BorderBrush="Black" BorderThickness="1"/>

                            <StackPanel Grid.Row="0" Grid.Column="0" Orientation="Vertical">
                                <Label Content="Print/Export for Directories?" HorizontalAlignment="Center" FontWeight="Bold"/>
                                <CheckBox IsChecked="{Binding SelectedProviderAccount.PrintOrExportForDirectories}" Margin="70,0,70,0" HorizontalAlignment="Center"/>
                            </StackPanel>

                            <StackPanel Grid.Row="0" Grid.Column="1"  Orientation="Vertical">
                                <Label Content="Provider ID:"  Margin="10,0,0,0" FontWeight="Bold"/>
                                <TextBox IsEnabled="False" Text="{Binding SelectedProviderAccount.ProviderId, Mode=TwoWay}" Margin="10,0,10,0"/>
                            </StackPanel>
                        </Grid>
                    </StackPanel>
                </Grid>
            </TabItem>


            <TabItem Header="Contacts" Background="Gray">
                <Grid Margin="10,40,10,10">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="100*"/>
                        <ColumnDefinition Width="100*"/>
                        <ColumnDefinition Width="100*"/>
                        <ColumnDefinition Width="100*"/>
                        <ColumnDefinition Width="200*"/>
                        <ColumnDefinition Width="100*"/>
                        <ColumnDefinition/>
                    </Grid.ColumnDefinitions>

                    <Grid.RowDefinitions>
                        <RowDefinition Height="300"/>
                    </Grid.RowDefinitions>

                    <Border BorderBrush="Gray" BorderThickness="1"  Grid.ColumnSpan="6" Margin="-5,-20,-5,-20"  />
                    <StackPanel Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="6">
                        <Grid>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="100*"/>
                                <ColumnDefinition Width="100*"/>
                                <ColumnDefinition Width="100*"/>
                                <ColumnDefinition Width="100*"/>
                                <ColumnDefinition Width="200*"/>
                                <ColumnDefinition Width="100*"/>
                                <ColumnDefinition/>
                            </Grid.ColumnDefinitions>

                            <Grid.RowDefinitions>
                                <RowDefinition Height="70" />
                                <RowDefinition Height="70" />
                                <RowDefinition Height="170"/>
                                <RowDefinition Height="70"/>
                                <RowDefinition Height="70"/>
                                <RowDefinition Height="70"/>
                                <RowDefinition Height="170"/>
                                <RowDefinition/>
                            </Grid.RowDefinitions>

                            <Border Grid.Row="0" Grid.Column="0" BorderBrush="Black" BorderThickness="1" Grid.ColumnSpan="6" Margin="10,5,10,5"/>
                            <StackPanel Grid.Row="0" Grid.Column="0"  Margin="20,10,10,10">
                                <Label Content="First Name:" FontWeight="Bold"/>
                                <TextBox Text="{Binding FirstName}"/>
                            </StackPanel>

                            <StackPanel Grid.Row="0" Grid.Column="1" Orientation="Vertical" Margin="10,10,10,10">
                                <Label Content="Last Name:" FontWeight="Bold"/>
                                <TextBox Text="{Binding LastName}"/>
                            </StackPanel>

                            <StackPanel Grid.Row="0" Grid.Column="2" Orientation="Vertical" Margin="10,10,10,10">
                                <Label Content="Title:"  FontWeight="Bold"/>
                                <TextBox Text="{Binding Title}"/>
                            </StackPanel>

                            <StackPanel Grid.Row="0" Grid.Column="3" Orientation="Vertical" Margin="10,10,10,10">
                                <Label Content="Contact type:" FontWeight="Bold"/>
                                <TextBox Text="{Binding ContactType}"/>
                            </StackPanel>

                            <StackPanel Grid.Row="0" Grid.Column="4" Orientation="Vertical" Margin="10,10,10,10">
                                <Label Content="Email:" FontWeight="Bold"/>
                                <TextBox Text="{Binding Email}"/>
                            </StackPanel>

                            <StackPanel Grid.Row="0" Grid.Column="5" Orientation="Vertical" Margin="10,10,20,10">
                                <Label Content="Department:" FontWeight="Bold"/>
                                <TextBox Text="{Binding Department}"/>
                            </StackPanel>

                            <Border Grid.Row="1" Grid.Column="0" BorderBrush="Black" BorderThickness="1" Grid.ColumnSpan="2" Margin="10,5,10,0"/>
                            <StackPanel Grid.Row="1" Grid.ColumnSpan="2" Height="60">
                                <StackPanel>
                                    <Grid Height="60">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition/>
                                            <ColumnDefinition/>
                                        </Grid.ColumnDefinitions>
                                        <Grid.RowDefinitions>
                                            <RowDefinition/>
                                            <RowDefinition/>
                                        </Grid.RowDefinitions>
                                        <Label Content="Reports To:" FontWeight="Bold" Margin="20,0,10,0" Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="2" VerticalAlignment="Bottom"/>
                                        <TextBox Text="{Binding ReportsToFirstName}" Grid.Row="1" Grid.Column="0" Margin="20,0,10,10"/>
                                        <TextBox Text="{Binding ReportsToLastName}" Grid.Row="1" Grid.Column="1" Margin="10,0,20,10"/>
                                    </Grid>
                                </StackPanel>
                            </StackPanel>

                            <Border Grid.Row="1" Grid.Column="2" BorderBrush="Black" BorderThickness="1" Grid.ColumnSpan="3" Margin="10,5,10,0"/>
                            <StackPanel Grid.Row="1" Grid.Column="2" Grid.ColumnSpan="3" Height="60" >
                                <StackPanel>
                                    <Grid Height="60">

                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition/>
                                            <ColumnDefinition/>
                                            <ColumnDefinition/>
                                            <ColumnDefinition/>
                                        </Grid.ColumnDefinitions>

                                        <Grid.RowDefinitions>
                                            <RowDefinition/>
                                            <RowDefinition/>
                                        </Grid.RowDefinitions>

                                        <Label Content="Phone:" Margin="20,0,10,0" Grid.Row="0" Grid.Column="0" VerticalAlignment="Bottom" FontWeight="Bold"/>
                                        <TextBox Text="{Binding Phone}" Grid.Row="1" Grid.Column="0" Margin="20,0,10,10"/>

                                        <Label Content="Ext:" Margin="10,0,20,0" Grid.Row="0" Grid.Column="1" VerticalAlignment="Bottom" FontWeight="Bold"/>
                                        <TextBox Text="{Binding PhoneExt}" Grid.Row="1" Grid.Column="1" Margin="10,0,20,10"/>

                                        <Label Content="Call:" Margin="20,0,10,0" Grid.Row="0" Grid.Column="2" VerticalAlignment="Bottom" FontWeight="Bold"/>
                                        <TextBox Text="{Binding Cell}" Grid.Row="1" Grid.Column="2" Margin="20,0,10,10"/>

                                        <Label Content="Fax:" Margin="10,0,20,0" Grid.Row="0" Grid.Column="3" VerticalAlignment="Bottom" FontWeight="Bold"/>
                                        <TextBox Text="{Binding Fax}" Grid.Row="1" Grid.Column="3" Margin="10,0,20,10"/>
                                    </Grid>
                                </StackPanel>
                            </StackPanel>

                            <StackPanel Grid.Row="1" Grid.Column="5" Height="60">
                                <StackPanel>
                                    <Grid Height="60">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition/>
                                            <ColumnDefinition/>
                                        </Grid.ColumnDefinitions>

                                        <Grid.RowDefinitions>
                                            <RowDefinition/>
                                            <RowDefinition/>
                                        </Grid.RowDefinitions>

                                        <Label Content="Contact ID:" Margin="20,0,10,0" Grid.Row="0" Grid.Column="0" VerticalAlignment="Bottom" FontWeight="Bold"/>
                                        <TextBox Text="{Binding ContactId}" IsEnabled="False" Grid.Row="1" Grid.Column="0" Margin="20,0,10,10"/>

                                        <Label Content="Provider ID:" Margin="10,0,20,0" Grid.Row="0" Grid.Column="1" VerticalAlignment="Bottom" FontWeight="Bold"/>
                                        <TextBox Text="{Binding ProviderId}" IsEnabled="False" Grid.Row="1" Grid.Column="1" Margin="10,0,20,10"/>
                                    </Grid>
                                </StackPanel>
                            </StackPanel>

                            <Border Grid.Row="2" Grid.Column="0" BorderBrush="Black" BorderThickness="1" Grid.ColumnSpan="3" Margin="10,10,10,10"/>
                            <StackPanel Grid.Row="2" Grid.Column="0" Grid.ColumnSpan="3" Height="150">
                                <Grid Height="150">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="120"/>
                                        <ColumnDefinition/>
                                    </Grid.ColumnDefinitions>
                                    <Grid.RowDefinitions>
                                        <RowDefinition/>
                                        <RowDefinition/>
                                        <RowDefinition/>
                                    </Grid.RowDefinitions>

                                    <Label Content="Address 1:" Margin="20,20,10,0" FontWeight="Bold"/>
                                    <TextBox Text="{Binding Address1}" Grid.Column="1" Margin="20,20,20,10"/>

                                    <Label Content="Address 2:" Grid.Row="1" Margin="20,20,10,0" FontWeight="Bold"/>
                                    <TextBox Text="{Binding Address2}" Grid.Row="1" Grid.Column="1" Margin="20,20,20,10"/>

                                    <Label Content="City,State,Zip:" Grid.Row="2" Margin="20,20,10,0" FontWeight="Bold"/>
                                    <StackPanel Grid.Row="2" Grid.Column="1">
                                        <Grid Height="50">
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="47*"/>
                                                <ColumnDefinition Width="27*"/>
                                                <ColumnDefinition Width="37*"/>
                                            </Grid.ColumnDefinitions>

                                            <TextBox Grid.Column="0" Text="{Binding City}" Margin="20,20,10,10"/>
                                            <ComboBox Grid.Column="1" Margin="0,20,0,8" 
                                                      ItemsSource="{Binding UsStates}">
                                                <ComboBox.ItemTemplate>
                                                    <DataTemplate>
                                                        <StackPanel Orientation="Horizontal">
                                                            <TextBlock Text="{Binding State}" Margin="0,0,10,0"/>
                                                            <TextBlock Text="{Binding StateName}"/>
                                                        </StackPanel>
                                                    </DataTemplate>
                                                </ComboBox.ItemTemplate>
                                            </ComboBox>


                                            <TextBox Text="{Binding ZipCode}" Grid.Column="2" Margin="10,20,20,10"/>
                                        </Grid>
                                    </StackPanel>
                                </Grid>
                            </StackPanel>

                            <Border Grid.Row="2" Grid.Column="3" BorderBrush="Black" BorderThickness="1" Grid.ColumnSpan="3" Margin="10,10,10,10"/>
                            <Label Content="Note:" Grid.Row="2" Grid.Column="3" Margin="10,10,10,20" FontWeight="Bold"/>
                            <TextBox Text="{Binding LastName}" Grid.Row="2" Grid.Column="3" Margin="114,20,20,20" Grid.ColumnSpan="3"/>
                        </Grid>
                    </StackPanel>
                </Grid>
            </TabItem>


            <TabItem Header="Credentialing" Background="Gray">
                <Grid Background="{DynamicResource {x:Static SystemColors.ControlBrushKey}}" RenderTransformOrigin="0.507,0.496">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="100*"/>
                        <ColumnDefinition Width="100*"/>
                        <ColumnDefinition Width="100*"/>
                        <ColumnDefinition Width="100*"/>
                        <ColumnDefinition Width="200*"/>
                        <ColumnDefinition Width="100*"/>
                        <ColumnDefinition/>
                    </Grid.ColumnDefinitions>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="30" />
                        <RowDefinition Height="200" />
                        <RowDefinition Height="30" />
                        <RowDefinition Height="300"/>
                        <RowDefinition Height="30" />
                        <RowDefinition Height="200"/>
                        <RowDefinition Height="150"/>
                        <RowDefinition />
                    </Grid.RowDefinitions>


                    <Border BorderBrush="Black" BorderThickness="1" Grid.Row="0" Margin="10,0,10,0" />
                    <Label Content="Basic Info:"  FontWeight="Bold" Grid.Row="0" Margin="10,0,10,0"/>

                    <Border BorderBrush="Black" BorderThickness="1" Grid.Row="1" Margin="10,0,10,10" Grid.ColumnSpan="4" />
                    <Label Content="Credentialing ID:" Margin="30,50,10,120" Grid.Row="1"/>
                    <TextBox Text="{Binding ProviderCredentials.CredentialingId}" Grid.Column="1"  Margin="10,50,50,120"  Grid.Row="1"/>

                    <Label Content="Delegated Credentialing:" Margin="30,100,10,70"  Grid.Row="1"/>
                    <CheckBox IsChecked="{Binding ProviderCredentials.DelegatedCredentialing}" Grid.Column="1"  Grid.Row="1" Margin="30,100,150,70" />

                    <Label Content="Entity Name:" Margin="30,150,10,20"  Grid.Row="1"/>
                    <TextBox Text="{Binding ProviderCredentials.CredentialingEntityId}" Grid.Column="1"  Grid.Row="1" Margin="10,150,30,20" Grid.ColumnSpan="3"/>

                    <Label Grid.Column="2" Content="Provider ID:" Margin="0,50,10,120" HorizontalAlignment="Right" Width="71"  Grid.Row="1"/>
                    <TextBox Text="{Binding ProviderCredentials.ProviderId}" Grid.Column="3"  Margin="50,50,30,120"  Grid.Row="1"/>

                    <Label Grid.Column="2" Content="Credentialing Entity ID:" Margin="0,100,10,70" HorizontalAlignment="Right" Width="131"  Grid.Row="1"/>
                    <TextBox Text="{Binding ProviderCredentials.CredentialingEntityId}" Grid.Column="3" Margin="50,100,30,70"  Grid.Row="1"/>

                    <Border BorderBrush="Black" BorderThickness="1" Grid.Row="0" Grid.Column="4" Margin="0,0,170,0"/>
                    <Label Content="Credentialing Application Dates:" Grid.Row="0" Grid.Column="4" FontWeight="Bold" Grid.ColumnSpan="2"/>
                    <Border Grid.Column="4" Grid.Row="1" BorderBrush="Black" BorderThickness="1" Margin="0,0,10,10"/>
                    <StackPanel Grid.Column="4" Margin="10,10,10,10" Grid.Row="1">
                        <Grid Height="180">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition/>
                                <ColumnDefinition/>
                            </Grid.ColumnDefinitions>
                            <Grid.RowDefinitions>
                                <RowDefinition/>
                                <RowDefinition/>
                                <RowDefinition/>
                                <RowDefinition/>
                            </Grid.RowDefinitions>

                            <Label Content="Date Sent:"  Grid.Row="1" Margin="10"/>
                            <Label Content="Date Received:"  Grid.Row="2" Margin="10"/>
                            <Label Content="Date Approved:"   Grid.Row="3" Margin="10"/>

                            <TextBox Text="{Binding ProviderCredentials.ApplicationSentDate}" Grid.Column="1" Grid.Row="1" Margin="10"/>
                            <TextBox Text="{Binding ProviderCredentials.ApplicationReceivedDate}" Grid.Column="1" Grid.Row="2" Margin="10"/>
                            <TextBox Text="{Binding ProviderCredentials.ApprovalDate}" Grid.Column="1" Grid.Row="3" Margin="10"/>
                        </Grid>

                    </StackPanel>

                    <Border BorderBrush="Black" BorderThickness="1" Grid.Row="2" Margin="10,0,10,0"/>
                    <Label Content="Application Checklist:"  FontWeight="Bold"  Grid.Row="2" Margin="10,0,10,0"/>
                    <StackPanel Grid.Row="3" Margin="10,0,10,10" Grid.ColumnSpan="5">
                        <Grid Height="280">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="50*"/>
                                <ColumnDefinition Width="80*"/>
                                <ColumnDefinition Width="100*"/>
                                <ColumnDefinition Width="80*"/>
                                <ColumnDefinition Width="100*"/>
                                <ColumnDefinition Width="50*"/>
                                <ColumnDefinition Width="100*"/>
                                <ColumnDefinition Width="196*"/>
                            </Grid.ColumnDefinitions>

                            <Grid.RowDefinitions>
                                <RowDefinition/>
                                <RowDefinition/>
                                <RowDefinition/>
                                <RowDefinition/>
                                <RowDefinition/>
                                <RowDefinition/>
                            </Grid.RowDefinitions>

                            <Border BorderBrush="Black" BorderThickness="1" Grid.ColumnSpan="8" Grid.RowSpan="6" />
